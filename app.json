{
  "expo": {
    "owner": "mobileapphcmut",
    "name": "EngFlash",
    "slug": "EngFlash-V2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.quangpro0166.EngFlashV2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.quangpro0166.EngFlashV2",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "enableFullScreenImage_legacy": true,
          "resizeMode": "cover",
          "backgroundColor": "#ff0000"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "url": "https://u.expo.dev/6e0d3528-99c9-417b-b33a-03b48acb9372"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e0d3528-99c9-417b-b33a-03b48acb9372"
      }
    }
  }
}
